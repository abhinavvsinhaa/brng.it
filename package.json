{
  "scripts": {
    "dev": "concurrently \"cd ./server && yarn dev\" \"cd ./client && yarn start\"",
    "client": "cd ./client && yarn start",
    "server": "cd ./server && yarn dev",
    "install --all": "cd ./server && yarn && cd ../client && yarn",
    "start": "concurrently \"cd ./server && yarn start\" \"cd ../client && yarn start\""
  },
  "dependencies": {
    "concurrently": "^7.2.2",
    "file-saver": "^2.0.5",
    "papaparse": "^5.3.2"
  },
  "devDependencies": {}
}
